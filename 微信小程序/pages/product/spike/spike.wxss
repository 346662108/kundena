/* pages/product/spike/spike.wxss */

page {
  background: #fff;
  color: #616264;
  font-size: 28rpx;
  line-height: 1.25;
}

.base-list-wrapper {
  margin-left: 10rpx;
}

.time-line {
  display: flex;
  height: 88rpx;
  position: relative;
}

.time-icon {
  width: 40rpx;
  height: 40rpx;
  background-image: url(data:image/png;base64,);
  background-size: 40rpx auto;
  background-position: 50% 50%;
  background-repeat: no-repeat;
  margin-top: 40rpx;
}

.time-show {
  margin: 34rpx 0 0 10rpx;
  display: flex;
}

.time-show::before, .time-show::after, .time-show .status {
  height: 54rpx;
  background-size: auto 54rpx;
}

.time-show::before, .time-show::after, .time-show .status {
  content: "";
  background-repeat: no-repeat;
}

.time-show::before {
  width: 26rpx;
  background-image: url(data:image/png;base64,);
}

.time-show::after {
  width: 18rpx;
  background-image: url(data:image/png;base64,);
}

.time-show .status {
  background-image: url(data:image/png;base64,);
  background-repeat: repeat-x;
  margin-left: -1px;
  font-size: 32rpx;
  text-align: center;
  line-height: 54rpx;
  color: #ce1a73;
}

.time-show text {
  font-weight: 700;
}

.line {
  width: 40rpx;
  position: relative;
}

.line:before {
  content: "";
  height: 112%;
  border-left: 1px solid #ce1a73;
  position: absolute;
  left: 50%;
}

.spike-goods {
  background-color: #fff;
  position: relative;
  display: flex;
  overflow: hidden;
}

.spike-goods:after {
  content: "";
  width: 100%;
  border-bottom: 1px solid #ededed;
  position: absolute;
  bottom: 0;
  left: 40rpx;
}

.spike-img {
  height: 200rpx;
  width: 200rpx;
  margin: 30rpx 0;
}

.spike-info {
  position: relative;
  flex: 1;
  margin: 30rpx 20rpx;
}

.left-time {
  display: flex;
  margin-top: 20rpx;
}

.spike-goods-name {
  font-size: 30rpx;
  color: #333;
  display: -webkit-box;
  height: 70rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.spike-buy {
  width: 100%;
  height: 110rpx;
  margin-top: 20rpx;
  overflow: hidden;
}

.spike-buy .item-status {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  height: 24rpx;
}

.spike-buy .item-status .status-bar {
  position: relative;
  width: 190rpx;
  height: 100%;
  color: #fff;
  background-color: rgba(206, 26, 115, 0.26);
  border-radius: 12rpx;
  overflow: hidden;
}

.spike-buy .item-status .status-bar .status-progress {
  position: relative;
  height: 100%;
  background-color: #ce1a73;
  border-radius: 22rpx;
}

.spike-buy .item-status .status-bar .status-five-percent {
  width: 10rpx;
  border-radius: 22rpx 0 0 22rpx;
}

.spike-buy .item-status .status-bar .status-more {
  width: 20rpx;
  border-radius: 22rpx;
}

.spike-buy .item-status .status-bar .status-soldrate {
  position: absolute;
  top: 0;
  left: 0;
  height: 100%;
  line-height: 24rpx;
  width: 100%;
  color: #fff;
  font-size: 20rpx;
  text-align: center;
  font-weight: 700;
}

.spike-buy .item-status .status-num {
  font-size: 24rpx;
  line-height: 24rpx;
  padding-right: 20rpx;
  color: #ce1a73;
}

.spike-buy .all-price {
  flex: 1;
  color: #ce1a73;
}

.spike-buy .all-price .sale-price {
  position: relative;
  font-weight: 700;
  display: inline-block;
}

.spike-buy .all-price .sale-price text:nth-child(1) {
  font-size: 32rpx;
}

.spike-buy .all-price .sale-price text:nth-child(2) {
  font-size: 44rpx;
}

.spike-buy .all-price .market-price {
  overflow: hidden;
  font-size: 24rpx;
  color: #9c9c9c;
  text-decoration: line-through;
  -webkit-text-decoration-color: #9c9c9c;
  text-decoration-color: #9c9c9c;
  padding-left: 10rpx;
  display: inline-block;
}

.btnClass:before {
  content: "\9a6c\4e0a\62a2";
  width: 170rpx;
  height: 64rpx;
  line-height: 64rpx;
  text-align: center;
  font-size: 30rpx;
  color: #fff;
  background: #ce1a73;
  border-radius: 10rpx;
  display: block;
}

.remind-purchase {
  font-size: 24rpx;
  line-height: 24rpx;
  color: #eeae0b;
}

.Processing .remind-purchase, .Comingsoon .item-status, .Grabend .item-status {
  display: none;
}

.Comingsoon .remind-purchase, .Grabend .remind-purchase {
  display: block;
}

/*已抢光*/

.Grabend .remind-purchase {
  color: #9c9c9c;
}

.Grabend .btnClass:before {
  content: "\5df2\62a2\5149";
  background: #e0e0e0;
  color: #9c9c9c;
}

/*即将开始*/

.Comingsoon .time-icon {
  background-image: url(data:image/png;base64,);
}

.Comingsoon .time-show::before {
  background-image: url(data:image/png;base64,);
}

.Comingsoon .time-show::after {
  background-image: url(data:image/png;base64,);
}

.Comingsoon .time-show .status {
  background-image: url(data:image/png;base64,);
  color: #eeae0b;
}

.Comingsoon .btnClass:before {
  content: "\5373\5c06\5f00\62a2";
  background: #eeae0b;
}

.Comingsoon .line:before {
  border-color: #eeae0b;
}
