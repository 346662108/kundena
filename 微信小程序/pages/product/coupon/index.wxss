/* pages/product/coupon/index.wxss */

page {
  background: #e8e8ed;
}

.coupon {
  margin: 0 25rpx;
}

.coupon__li {
  height: 180rpx;
  background: #fff;
  border-radius: 6rpx;
  font-size: 26rpx;
  line-height: 1.25;
  margin-top: 20rpx;
  display: flex;
  overflow: hidden;
}

.coupon__info {
  flex: 1;
  padding: 10rpx 20rpx;
}

.coupon__num {
  color: #47c1c4;
  margin-bottom: 10rpx;
}

.coupon__num text:nth-child(1) {
  font-size: 24rpx;
}

.coupon__num text:nth-child(2) {
  font-size: 56rpx;
  margin-right: 10rpx;
}

.coupon__num text.freemail{
  font-size: 40rpx;
  font-weight: 600;
}

.coupon__text {
  color: #333;
}

.coupon__text view {
  font-size: 24rpx;
  margin-top: 10rpx;
  display: flex;
}

.coupon__text .name {
  border: 1rpx solid #47c1c4;
  border-radius: 3rpx;
  color: #47c1c4;
  padding: 0 5rpx;
  font-size: 22rpx;
  margin-right: 10rpx;
}

.coupon__text .text {
  width: 100%;
  max-height: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  margin-left: 5rpx;
  flex: 1;
}

.coupon__text .mail {
  color: #47c1c4;
  font-size: 24rpx;
  margin-left: 10rpx;
}

.coupon__date {
  width: 160rpx;
  line-height: 180rpx;
  background: #47c1c4;
  color: #fff;
  font-size: 28rpx;
  text-align: center;
  position: relative;
}


.receive__state{
  width: 120rpx;
  height: 88rpx;
  background: url(data:image/png;base64,);
  background-size: 100% auto;
  position: absolute;
  top: 0;
  left: 50%;
  transform: translateX(-50%) translateY(0);
}